{
  "name": "chronoscope-react",
  "version": "2.0.0",
  "type": "module",
  "description": "ChronoScope Date/Time Range Picker â€” A hooks-first, design-agnostic, fully headless Grafana-style date-time range selector for React. Ships with Tailwind and Bootstrap presets.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./hooks": {
      "types": "./dist/types/hooks/index.d.ts",
      "import": "./dist/esm/hooks/index.js",
      "require": "./dist/cjs/hooks/index.js"
    },
    "./presets/tailwind": {
      "types": "./dist/types/presets/tailwind/index.d.ts",
      "import": "./dist/esm/presets/tailwind/index.js",
      "require": "./dist/cjs/presets/tailwind/index.js"
    },
    "./presets/bootstrap": {
      "types": "./dist/types/presets/bootstrap/index.d.ts",
      "import": "./dist/esm/presets/bootstrap/index.js",
      "require": "./dist/cjs/presets/bootstrap/index.js"
    },
    "./presets/unstyled": {
      "types": "./dist/types/presets/unstyled/index.d.ts",
      "import": "./dist/esm/presets/unstyled/index.js",
      "require": "./dist/cjs/presets/unstyled/index.js"
    },
    "./utils": {
      "types": "./dist/types/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig.build.json",
    "build:types": "tsc -p tsconfig.build.json",
    "dev": "rollup -c -w",
    "lint": "eslint src/",
    "test": "vitest",
    "test:unit": "vitest run --project unit",
    "test:coverage": "vitest run --project unit --coverage",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chronoscope/chronoscope-react"
  },
  "keywords": [
    "react",
    "datepicker",
    "daterange",
    "datetime",
    "time-range",
    "grafana",
    "headless",
    "hook",
    "hooks-first",
    "unstyled",
    "tailwind",
    "bootstrap",
    "design-agnostic",
    "customizable",
    "render-prop",
    "chronoscope"
  ],
  "author": "ChronoScope Contributors",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-a11y": "^10.2.10",
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/addon-onboarding": "^10.2.10",
    "@storybook/addon-vitest": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@testing-library/react": "^16.3.2",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/browser": "^3.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "jsdom": "^27.0.1",
    "playwright": "^1.58.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark-gfm": "^4.0.1",
    "rollup": "^4.0.0",
    "storybook": "^10.2.10",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  }
}
